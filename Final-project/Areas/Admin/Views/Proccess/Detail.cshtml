
@namespace Final_project.ViewModels.Proccess

@model ProccessDetailVM

<div class="container mt-5">
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Title</th>
                <th scope="col">Description</th>
                <th scope="col">Number</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>@Model.Title</td>
                <td>@Model.Description</td>
                <td>@Model.Number</td>
            </tr>
        </tbody>
    </table>
    <a asp-action="Index" class="btn btn-primary text-white">Back</a>
</div>
